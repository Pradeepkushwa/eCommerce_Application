<%= form_tag import_csv_admin_products_path, multipart: true do %>
  <table>
    <tr>
      <td>
        <%= label_tag :file, 'Select a CSV File:' %>
      </td>
      <td>
        <%= file_field_tag 'product[csv_file]' %>
      </td>
    </tr>
    <tr>
      <td colspan="2">
        <%= submit_tag 'Submit', class: 'btn btn-primary' %>
      </td>
    </tr>
  </table>
<% end %>